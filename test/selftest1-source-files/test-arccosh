"Testing arccosh"

clear

T = prefixform(arccosh(x))
check(T="(arccosh x)")

T = prefixform(arccosh(cosh(x)))
check(T="x")

T = prefixform(arccosh(0))
check(T="(arccosh 0)")

T = prefixform(arccosh(1))
check(T="0")

T = prefixform(arccosh(1.0))
check(T="0.0")

T = prefixform(arccosh(-1))
check(T="(arccosh -1)")

T = prefixform(arccosh(-1.0))
check(T="(* 3.14159 (^ -1 1/2))")

T = prefixform(float(arccosh(1+i)))
check(T="(+ 1.06128 (* 0.904557 (^ -1 1/2)))")

for(x,-12,12,
  d = float(arccosh(x/12)) - arccosh(float(x/12)),
  check(abs(d) < 1.0 10^(-6))
)
