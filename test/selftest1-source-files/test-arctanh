"Testing arctanh"

clear

T = prefixform(arctanh(x))
check(T="(arctanh x)")

T = prefixform(arctanh(tanh(x)))
check(T="x")

T = prefixform(arctanh(0))
check(T="0")

T = prefixform(arctanh(1))
check(T="(arctanh 1)")

T = prefixform(arctanh(-1))
check(T="(arctanh -1)")

T = prefixform(arctanh(0.5))
check(T="0.549306")

T = prefixform(arctanh(-0.5))
check(T="-0.549306")

T = prefixform(float(arctanh(1+i)))
check(T="(+ 0.402359 (* 1.01722 (^ -1 1/2)))")
